      "data": {
        "type": "content_block_delta",
        "index": 1,
        "delta": { "type": "text_delta", "text": "```tsx\nconst [count, setCount] = useState(0);\n// Read: count\n// Update: setCount(count + 1)\n```\n\n" }
      }
    },
